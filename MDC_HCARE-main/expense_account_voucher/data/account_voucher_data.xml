<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <data>

        <record model="ir.sequence" id="seq_voucher_receipt">
            <field name="name">Receipt Voucher Sequence</field>
            <field name="code">voucher.receipt</field>
            <field name="prefix">RV%(range_year)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <record model="ir.sequence" id="seq_voucher_payment">
            <field name="name">Payment Voucher Sequence</field>
            <field name="code">voucher.payment</field>
            <field name="prefix">PV%(range_year)s</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>

</odoo>