<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <record id="view_partner_form" model="ir.ui.view">
            <field name="name">res.partner.form</field>
			<field name="model">res.partner</field>
            <field name="inherit_id" ref="pragtech_dental_management.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='amt_paid_by_insurance']" position="before">
                    <field name="sub_insurar_ids" widget="many2many_tags"
                           options="{'no_create': True, 'no_create_edit':True}"
                           attrs="{ 'invisible':[('is_insurance_company', '=', False)]}" />
                </xpath>
            </field>
    </record>

</odoo>
