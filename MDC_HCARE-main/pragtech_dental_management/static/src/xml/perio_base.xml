	<div  class="flex-container">

     	<div class = "col-2">
			<div style="border:1px; width:250px;height:540px;overflow:scroll;">
				<script>
					$(document).ready(function() {
						$('.deleteicon').click(function() {
							$(this).prev('input').val('').trigger('change').focus();
						});
					});
				</script>
				<style>
					span.deleteicon {
						position: relative;
						top: -17px;
    					left: 93%;
						cursor: pointer;
					}
					
				</style>
				<input type="text" style="margin-top:5px;"
					   id="treatment_search"
					   placeholder="Search treatment"
				/>
				<span class="deleteicon glyphicon glyphicon-remove" />

				<h2>Procedures</h2>
				<div id = "categ_list" class = "categs">
					<div id = "total_list_div" class="panel panel-default">
					</div>
				</div>

			</div>
		</div>

		<div class="col-1 ">
			<div class="tabarea">
				<div class = "mouth_selection">
					<!--patient details-->
					<div class="patient_details"></div>

						<div class="charttype">
							<input type="radio" id="adult" class="chartselect" value="adult" name="chartoption" checked="checked"/>
							<label for="adult">Adult</label>

							<input type="radio" id="child" class="chartselect" value="child" name="chartoption"/>
							<label for="child">Child</label>
						</div>

						<div>
							<input type="checkbox" value="None" id="select_full_mouth_child" name="check" class="hidden"/>
							<input type="checkbox" value="None" id="select_full_mouth" name="check" class=""/>
							<label for="select_full_mouth">Full Mouth</label>
						</div>
						<div>
							<input type="checkbox" id="select_upper_mouth_child" class="jaw hidden" name="mouthselection" value="uppermouth_child"/>
							<input type="checkbox" id="select_upper_mouth" class="jaw" name="mouthselection" value="uppermouth"/>
							<label for="select_upper_mouth">Upper Jaw</label>
						</div>

						<div>
							<input type="checkbox" id="select_lower_mouth_child" class="jaw hidden" name="mouthselection" value="lowermouth_child"/>
							<input type="checkbox" id="select_lower_mouth" class="jaw" name="mouthselection" value="lowermouth"/>
							<label for="select_lower_mouth">Lower Jaw</label>
						</div>
